{
    "name":"bright-cloud-studio/contao-form-to-form",
    "description":"Allows one form's fields to be pre-filled with values submitted from another.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "form", "pre-fill", "prefill", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/contao_form_to_form/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_form_to_form": "system/modules/contao_form_to_form"
            }
        }
    },
    "minimum-stability": "dev"
}
